{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/public/index.html"
        },
        {
            "src": "/get_formats",
            /* DEST: Seedha file name ke bajaye /api/index.py dein */
            "dest": "/api/index.py"
        },
        {
            "src": "/download_specific",
            /* DEST: Seedha file name ke bajaye /api/index.py dein */
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}